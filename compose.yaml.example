version: '3'

services:
  app:
    image: sino
    networks:
      - coinsino
    environment:
      treakey: ${treakey}
      injkey: ${injkey}
      opkey: ${opkey}
      apiKey: ${apiKey}
      authDomain: ${authDomain}
      projectId: ${projectId}
      storageBucket: ${storageBucket}
      messagingSenderId: ${messagingSenderId}
      appId: ${appId}
      measurementId: ${measurementId}
      jwt_secret: ${jwt_secret}
      entryKey: ${entryKey}
    ports:
      - '80:3000'
networks:
  coinsino:
    external: true